{
  "expo": {
    "name": "simdi",
    "slug": "simdi",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon":  "./assets/images/taxi_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/taxi_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mamgain2611.new_simri_app",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAopathNjAm8ycAgsVLkJ-no21SN6BMSTM"
        }
      }
    },
    
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "./config_1",

      [
        "expo-splash-screen",
        {
          "image": "./assets/images/taxi_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bf6580db-3aed-423c-a1d8-b81f9c0041ee"
      },
        "googleMapsApiKey": "AIzaSyAopathNjAm8ycAgsVLkJ-no21SN6BMSTM"

    }
  }
}

